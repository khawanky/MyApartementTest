<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.us.singledigits.myapartment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menu.help.HelpInfoActivity"></activity>
        <activity android:name=".menu.myprofile.MyprofileActivity" />
        <activity android:name=".menu.myapartment.MyapartmentActivity" />
        <activity android:name=".menu.help.HelpActivity" />
        <activity android:name=".menu.about.AboutActivity" />
        <activity android:name=".menu.menu_list.MenuActivity" />
        <activity android:name=".create_account.password.EnterPasswordActivity" />
        <activity android:name=".create_account.email.EmailCheckerActivity" />
        <activity android:name=".create_account.vcode.VerificationCodeActivity" />
        <activity android:name=".create_account.information.AccountInfoActivity" />
        <activity android:name=".home.HomePageActivity" />
        <activity
            android:name=".login.ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>